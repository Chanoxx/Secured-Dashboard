
services:
  mysql:
    image: mysql:8.0
    environment:
      MYSQL_ROOT_PASSWORD: trailbaze_root_password
      MYSQL_DATABASE: trailblaze_db
      MYSQL_USER: trailblaze_user
      MYSQL_PASSWORD: trailblaze_password
    ports:
      "3306:3306"
    volume: 
      mysql_data:/var/lib/mysql
    networks:
      trailblaze_network
